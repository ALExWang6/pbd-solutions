public class UseVar {

    public static void main (String[] args) {
    
        int room = 113;
        double eValue = 2.1478;
        String subject = "Com Science";
        
        System.out.println("This is room #" + room);
        System.out.println("'e' is close to " + eValue);
        System.out.println("I am learnig " + subject);
 
    }       
        
}
